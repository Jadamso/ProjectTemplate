@article{ECPO12066,
    author     = {Costa-Font, Joan and De-Albuquerque, Filipe and Doucouliagos, Hristos},
    title      = {Does Inter-jurisdictional Competition Engender a {\textquotedblleft}Race to the Bottom{\textquotedblright}? A Meta-Regression Analysis},
    keywords   = {paper2},
    volume     = {27},
    number     = {3},
    pages      = {488--508},
    abstract   = {A growing literature documents the existence of strategic
                political reactions to public expenditure between rival
                jurisdictions. These interactions can potentially create a
                downward expenditure spiral (``race to the bottom'') or a
                rising expenditure spiral (``race to the top''). However, in
                the course of identifying the existence of such interactions
                and ascertaining their underlying triggers, the empirical
                evidence has produced markedly heterogeneous findings. Most of
                this heterogeneity can be traced back to study design and
                institutional differences. This article contributes to the
                literature by applying meta-regression analysis to quantify the
                magnitude of strategic inter-jurisdictional expenditure
                interactions, controlling for study, and institutional
                characteristics. We find several robust results beyond
                confirming that jurisdictions do engage in strategic
                expenditure interactions, namely that strategic interactions:
                (i) are weakening over time, (ii) are stronger among
                municipalities than among higher levels of government, and
                (iii) appear to be more influenced from tax competition than
                yardstick competition, with capital controls and fiscal
                decentralization shaping the magnitude of fiscal
                interactions.},
    publisher = {Wiley-Blackwell},
    date      = {2015-08},
    doi       = {10.1111/ecpo.12066},
    issn      = {1468-0343},
    journal   = {Economics {\&} Politics},
    url       = {http://onlinelibrary.wiley.com/doi/10.1111/1475-6765.12066/pdf},
}


