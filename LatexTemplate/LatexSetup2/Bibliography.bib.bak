@inproceedings{INQ,
    author       = {Iyigun, Murat and Nunn, Nathan  and Qian, Nancy},
    title        = {Agricultural Productivity, Conflict and State Size: Evidence from Potatoes, 1400--1900},
    keywords     = {paper1,paper2, Conflict, State Capacity, Natural Resources},
    date         = {2015-12},
    booktitle    = {2015 Political Economy Conference},
    organization = {Program for Economic Research and the Department of Political Science at Columbia University},
    note         = {Preliminary Draft},
    url          = {http://aida.wss.yale.edu/~nq3/NANCYS_Yale_Website/resources/papers/PAPER_20150507_nq.pdf},
    abstract     = {This paper investigates the long-run effect of a permanent rise
    in agricultural productivity on conflict. To overcome data limitations, we
    construct a new digitized and geo-referenced dataset on all conflicts with more
    than 32 combat mortalities in Europe, the Near East and North Africa, for
    1400-1900. For plausibly exogenous variation in a permanent rise in
    agricultural productivity, we exploit the introduction of potatoes from the
    Americas to the “Old World” after the Columbian Exchange. The results show that
    the introduction of potatoes reduced conflict. This effect persisted for two
    centuries.  These results are consistent with several conventional theories of
    conflict.},
}

@article{KS,
    author     = {Konrad, Kai A. and Skaperdas, Stergios},
    title      = {The market for protection and the origin of the state},
    keywords   = {paper2},
    volume     = {50},
    number     = {2},
    pages      = {417--443},
    date       = {2010-10},
    publisher  = {Springer Nature},
    doi        = {10.1007/s00199-010-0570-x},
    issn       = {1432-0479},
    journal    = {Economic Theory},
    abstract   = {We examine a stark setting in which security or protection can
    be provided by self-governing groups or by for-profit entrepreneurs (kings,
    kleptocrats, or mafia dons). Although self-governance is best for the
    population, it faces problems of long-term viability. Typically, in providing
    security, the equilibrium market structure involves competing lords, a
    condition that leads to a tragedy of coercion: all the savings from the
    provision of collective protection are dissipated and welfare can be as low as,
    or even lower than, in the absence of the state. Thus, we explain the tendency
    towards autocracy both in history, before the appearance of modern
    representative governance, and in many low-income countries in modern times.},
}

@Article{Kornprobst,
    author     = {Kornprobst, Markus},
    title    = {The management of border disputes in African regional sub-systems: comparing West {Africa} and the Horn of {Africa}},
    keywords   = {paper2},
    volume     = {40},
    pages    = {369--393},
    date     = {2002-09},
    doi      = {10.1017/S0022278X02003968},
    issn     = {1469-7777},
    issue    = {03},
    journal    = {The Journal of Modern African Studies},
    pagetotal   = {25},
    url      = {http://journals.cambridge.org/article_S0022278X02003968},
    abstract   = { ABSTRACT In Africa, the management of border disputes varies
    from sub-region to sub-region. Most puzzling is the difference between West
    Africa and the Horn of Africa. In the latter, border disputes are much more
    likely to escalate into war than in the former. Seeking to solve this puzzle,
    this study focuses on the territorial integrity norm. It departs from existing
    accounts of this norm in two ways: first, it does not choose the region but the
    sub-region as the level of analysis. Second, it does not isolate the
    territorial integrity norm from its social context but analyses the interplay
    of the norm with the social structure in which it is embedded. It concludes
    that the territorial integrity norm in West Africa is part of a social
    structure different from that in the Horn of Africa. It is this difference that
    explains the different patterns of conflict management in the two sub-regions.
},
}

