@book{Dig,
    title     = {Statistical Analysis of Spatial and Spatio-Temporal Point Patterns},
    author    = {Diggle, Peter J.},
    keywords  = {paper1},
    publisher = {Taylor \& Francis},
    series    = {Chapman \& Hall/CRC Monographs on Statistics \& Applied Probability},
